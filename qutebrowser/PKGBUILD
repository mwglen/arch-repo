license=('GPL')
source=('config.py')
md5sums=('SKIP')
depends=('qutebrowser' 'python-qutescript-git')

package() { 
    mkdir -p $XDG_CONFIG_HOME/qutebrowser
    cp config.py $XDG_CONFIG_HOME/qutebrowser
}
