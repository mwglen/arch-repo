pkgname=mwglen-dunst
pkgver=1
pkgrel=1
pkgdesc="A dunst configuration"
arch=('x86_64')
url=https://github.com/mwglen/arch-linux
license=('GPL')
md5sums=('SKIP')

makedepends=('rsync')
depends=('dunst' 'libnotify')

package() { 
    rsync --append home $HOME 
    rsync config $XDG_CONFIG_HOME
}
