pkgname=mwglen-dunst
pkgver=1
pkgrel=1
pkgdesc="A dunst configuration"
arch=('x86_64')
url=https://github.com/mwglen/arch-config/dunst
license=('GPL')
source=('dunstrc')
md5sums=('SKIP')
depends=('dunst' 'libnotify')

package() {
    mkdir -p $XDG_CONFIG_HOME/dunst
    cp dunstrc $XDG_CONFIG_HOME/dunst
}
