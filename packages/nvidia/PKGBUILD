pkgname=nvidia-mwglen
pkgver=1
pkgrel=$(date +%s)
pkgdesc="Nvidia optimus setup"
arch=('x86_64')
url=https://github.com/mwglen/arch-config/nvidia
license=('GPL')
md5sums=('SKIP')

#-------------------------------------------------------------------
# Base Packages
depends=('nvidia' 'optimus-manager-qt')
#-------------------------------------------------------------------

package() {
    mkdir -p $pkgdir/etc/optimus-manager
    cp optimus-manager.conf $pkgdir/etc/optimus-manager
}
