pkgname=mwglen-emacs
pkgver=1
pkgrel=$(date +%s)
pkgdesc="An emacs configuration"
arch=('x86_64')
url=https://github.com/mwglen/arch-linux
license=('GPL')
source=('doom-moonless-theme.el')
md5sums=('SKIP')
depends=('emacs28-git')

package() {
    mkdir -p $HOME/emacs.d
    cp doom-moonless-theme.el $HOME/emacs.d
}
